<script type="text/javascript">
    $(window).load(function(){
        $(".done").click(function(){
            var this_li_ind = $(this).parent().parent("li").index();
            if($('.payment-wizard li').hasClass("jump-here")){
                $(this).parent().parent("li").removeClass("active").addClass("completed");
                $(this).parent(".wizard-content").slideUp();
                $('.payment-wizard li.jump-here').removeClass("jump-here");
            }else{
                $(this).parent().parent("li").removeClass("active").addClass("completed");
                $(this).parent(".wizard-content").slideUp();
                $(this).parent().parent("li").next("li:not('.completed')").addClass('active').children('.wizard-content').slideDown();
            }
        });

        $('.payment-wizard li .wizard-heading').click(function(){
            if($(this).parent().hasClass('completed')){
                var this_li_ind = $(this).parent("li").index();
                var li_ind = $('.payment-wizard li.active').index();
                if(this_li_ind < li_ind){
                    $('.payment-wizard li.active').addClass("jump-here");
                }
                $(this).parent().addClass('active').removeClass('completed');
                $(this).siblings('.wizard-content').slideDown();
            }
        });

        $('#getParts').click(function(){
            var liquid = $('#liquid').val();
            var tolerance= $('#tolerance').val();
            var combo = $('#servoSyringe').find(":selected").text();
            if (combo=="Recommended"){
                localStorage.syringe= 10;
                localStorage.servo= 'Hitec';
            }
            else if (combo=="Lowest cost"){
                localStorage.syringe= 60;
                localStorage.servo= 'Parallax';
            }
            else if (combo=="Highest tolerance"){
                localStorage.syringe= 3;
                localStorage.servo= 'Datan';
            }
            else {
                //Custom
                localStorage.syringe= 'custom';
                localStorage.servo= 'custom';

            }
            $('#orderList').val(
                    'Quantity, Item, Explanation, Link' +'\n'
                    +'Electric components,,,' +'\n'
                    + '1, Arduino Control Shield,'+ 'To connect and power multiple servos' +'\n'
                    + '1, Arduino Uno,'+ 'To control the microfluidic chip' +'\n'
                    + '1, Power plugin for the arduino board,'+ 'For powering the servos' +'\n'
                    + '1, Capacitor,'+ 'To prevent the servos from getting too much power' +'\n'
                    + '1, Connecting Wires,'+ 'To connect the servos to the arduino' +'\n'
                    + '1, Arduino Uno,'+ 'To control the microfluidic chip' +'\n'
                    + '1, Stacking headers,'+ 'To connect the servo wires to the arduino' +'\n'
                    + '1, Laptop power supply,'+ 'To power the servos' +'\n'

                    +'Servo and syringe components,,,' +'\n'
                    + localStorage.pumps +',' + localStorage.syringe + 'ml Syringes, To push liquids and open/close valves' +'\n'
                    + localStorage.pumps +',' + localStorage.servo + ' servos, To open and close syringes' +'\n'
                    + localStorage.pumps*5 +', Machine Screw: 4-40 5/8",' + 'For screwing in servo and syringe attachment' +'\n'
                    + localStorage.pumps*1 + ', Small machine screw,' + 'For screwing in servo arm' +'\n'
                    + localStorage.pumps*1 +', Small nut,' + 'For screwing in servo arm' +'\n'
                    + localStorage.pumps*5 +', Nut: 4-40,' + 'For screwing in servo and syringe' +'\n'

                    +'Hardware components,,,' +'\n'
                    + '1, CNC Mill,'+ 'To create the microfluidic chip' +'\n'
                    + '1, 3D Printer,'+ 'To build the setup to control the microfluidic chip' +'\n'
                    + '1, 3D Printing material roll,' + 'For 3D printing setup' +'\n'
                    + '1, 3D Printing support roll,' + 'For 3D printing setup' +'\n'
                    + '2, 1/4 inch polycarbonate sheet,' + 'For microfluidic chip' +'\n'
                    + '1, 1/4 inch endmill,'+ 'For CNC mill' +'\n'
                    + localStorage.pumps*6 + ', Woodscrews 29 8-1",' + 'For mounting onto board' +'\n'

                    +'Optional components,,,' +'\n'
                    + '1, Peristaltic pump,'+ 'To pump fluids through the microfluidic chip' +'\n'
                    + '2, Machine Screw: 4-40 5/8",' + 'For screwing in the peristaltic pump' +'\n'
                    + '2, Nut: 4-40,' + 'For screwing in the peristaltic pump' +'\n'
            );
            var a = document.createElement('a');
            with (a) {
                href='data:text/csv;base64,' + btoa(document.getElementById('orderList').value);
                download='orderlist.csv';
            }
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
        });

