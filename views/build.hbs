            }
            else {
                toastr.info('Volume value accepted.');
            }
        });
        $('#checkTolerance').click(function(){
            var tolerance = $('#tolerance').val();
            if (tolerance<0) {
                toastr.warning('Tolerance must be greater than 0.1! Please enter again.');
            }
            else {
                toastr.info('Tolerance value accepted.');
            }
        });

        $('#specs').click(function(){
            var tolerance = $('#tolerance').val();
            var liquid = $('#liquid').val();
            localStorage.precision= tolerance;
            localStorage.volume = liquid;
            if (liquid<=0 || liquid >100 || tolerance<0) {
                toastr.warning('Error! Please enter values to proceed');
            }
            else {
                var this_li_ind = $(this).parent().parent("li").index();
                if($('.payment-wizard li').hasClass("jump-here")){
                    $(this).parent().parent("li").removeClass("active").addClass("completed");
                    $(this).parent(".wizard-content").slideUp();
                    $('.payment-wizard li.jump-here').removeClass("jump-here");
                }else{
                    $(this).parent().parent("li").removeClass("active").addClass("completed");
                    $(this).parent(".wizard-content").slideUp();
                    $(this).parent().parent("li").next("li:not('.completed')").addClass('active').children('.wizard-content').slideDown();
                }
            }
            calculateRecommended();
        });

        $('#customize').click(function(){
            var tolerance = $('#tolerance').val();
            var liquid = $('#liquid').val();
            if (liquid>=0 && liquid <10) {
               $('#suggestion').val('Suggestion: Choose a 10ml syringe and a servo with '+tolerance+ ' tolerance.');
            }
            else if (liquid>=10 && liquid <25){
                $('#suggestion').val('Suggestion: Choose a 25ml syringe and a servo with '+tolerance+ ' tolerance.');
            }
            else if (liquid>=25 && liquid <50){
                $('#suggestion').val('Suggestion: Choose a 50ml syringe and a servo with '+tolerance+ ' tolerance.');
            }
            else{
                $('#suggestion').val('Suggestion: Choose a 100ml syringe and a servo with '+tolerance+ ' tolerance.');
            }


        });


    })
</script>




