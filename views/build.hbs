<script src="http://code.jquery.com/jquery-1.11.3.min.js"></script>
    <link rel="stylesheet" type="text/css" href="http://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css"/>
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
<script type="text/javascript">
    $(window).load(function(){
        $(".done").click(function(){
            var this_li_ind = $(this).parent().parent("li").index();
            if($('.payment-wizard li').hasClass("jump-here")){
                $(this).parent().parent("li").removeClass("active").addClass("completed");
                $(this).parent(".wizard-content").slideUp();
                $('.payment-wizard li.jump-here').removeClass("jump-here");
            }else{
                $(this).parent().parent("li").removeClass("active").addClass("completed");
                $(this).parent(".wizard-content").slideUp();
                $(this).parent().parent("li").next("li:not('.completed')").addClass('active').children('.wizard-content').slideDown();
            }
        });

        $('.payment-wizard li .wizard-heading').click(function(){
            if($(this).parent().hasClass('completed')){
                var this_li_ind = $(this).parent("li").index();
                var li_ind = $('.payment-wizard li.active').index();
                if(this_li_ind < li_ind){
                    $('.payment-wizard li.active').addClass("jump-here");
                }
                $(this).parent().addClass('active').removeClass('completed');
                $(this).siblings('.wizard-content').slideDown();
            }
        });

        $('#getParts').click(function(){
            var liquid = $('#liquid').val();
            var tolerance= $('#tolerance').val();
            var combo = $('#servoSyringe').find(":selected").text();
            $('#orderList').val(
                    'Volume of liquid required to dispense in ml: '  + liquid + '\n'
                    + 'Tolerance of required servos: ' + tolerance + '\n'
                    + 'You have selected the ' + combo + ' Servo and Syringe Combo'
            );
        });

        $('#checkVolume').click(function(){
            var liquid = $('#liquid').val();
            if (liquid<=0 || liquid >100) {
                toastr.warning('Volume of liquid must be between 0 and 100! Please enter again.');
            }
            else {
                toastr.info('Volume value accepted.');
            }
        });
        $('#checkTolerance').click(function(){
            var tolerance = $('#tolerance').val();
            if (tolerance<0.1) {
                toastr.warning('Tolerance must be greater than 0.1! Please enter again.');
            }
            else {
                toastr.info('Tolerance value accepted.');
            }
        });

        $('#specs').click(function(){
            var tolerance = $('#tolerance').val();
            var liquid = $('#liquid').val();
            if (liquid<=0 || liquid >100 || tolerance<0.1) {
                toastr.warning('Error! Please enter values to proceed');
            }
            else {
                var this_li_ind = $(this).parent().parent("li").index();
                if($('.payment-wizard li').hasClass("jump-here")){
                    $(this).parent().parent("li").removeClass("active").addClass("completed");
                    $(this).parent(".wizard-content").slideUp();
                    $('.payment-wizard li.jump-here').removeClass("jump-here");
                }else{
                    $(this).parent().parent("li").removeClass("active").addClass("completed");
                    $(this).parent(".wizard-content").slideUp();
                    $(this).parent().parent("li").next("li:not('.completed')").addClass('active').children('.wizard-content').slideDown();
                }
            }
        });

        $('#customize').click(function(){
            var tolerance = $('#tolerance').val();
            var liquid = $('#liquid').val();
            if (liquid>=0 && liquid <10) {
               $('#suggestion').val('Suggestion: Choose a 10ml syringe and a servo with '+tolerance+ ' tolerance.');
            }
            else if (liquid>=10 && liquid <25){
                $('#suggestion').val('Suggestion: Choose a 25ml syringe and a servo with '+tolerance+ ' tolerance.');
            }
            else if (liquid>=25 && liquid <50){
                $('#suggestion').val('Suggestion: Choose a 50ml syringe and a servo with '+tolerance+ ' tolerance.');
            }
            else{
                $('#suggestion').val('Suggestion: Choose a 100ml syringe and a servo with '+tolerance+ ' tolerance.');
            }

        });


    })
</script>

