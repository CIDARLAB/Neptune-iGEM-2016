</nav>
<div class="neptune-sideleft">
    <h2> Sensors </h2>
    <div>
        Content that will go in the div. but also not flow into the other part.
    </div>
</div>

<div id= "neptune-data-container" class="neptune-container">

</div>
<!--Footer-->
<div class="neptune-footer">
        <label for="ports" style="color:#777777">Serial Ports Available</label>
        <select id="ports">
            {{#each serialPorts}} <!-- // Note the reference to the customers object​-->
                <option value={{this.comName}}> name: {{ this.comName }} </option>​
            {{/each}}
        </select>
    <div class="serial-communication-buttons">
            <button class="btn btn-default serial-connection-button" id="begin-comm"> Open Comm</button>
            <button class="btn btn-default connections serial-connection-button" id="end-comm"> Close Comm</button>
            <button class="btn btn-default connections serial-connection-button" id="read-button"> Take Reading</button>
            <button class="btn btn-default connections serial-connection-button" id="read-continuously-button"> Take Continuous Reading</button>
    </div>
</div>

