<body class="content">

<nav class="neptune-navbar">
    <div class="container-fluid">
        <div class="navbar-header" >
            <div class="neptune-logo">
            <img height="50" width="50" src="../images/NeptuneLogo.png">
            </div>
            <div class="neptune-brand">
                <a class="neptune-text" style="font-family: 'Open Sans'; font-size: 20px;" href="#">NEPTUNE</a>
            </div>
        </div>
        <ul class="nav navbar-nav">
            <!--<li class="active"><a href="#">Home</a></li>-->
