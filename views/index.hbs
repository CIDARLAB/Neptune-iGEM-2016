<script type="text/javascript">
    function create_user()
    {
        localStorage.USERNAME = (document.getElementById('exampleInputEmail2').value);
        localStorage.PASS     = (document.getElementById('exampleInputPassword2').value);
        $.post('/api/Create_User',{username:localStorage.USERNAME,password:localStorage.PASS},function(id,error)
        {
            localStorage.ID = id;
            login(id);
        });
    }
</script>
<script type="text/javascript">
    function login(id)
    {
        $.post('/api/Query_User',{id:id,lookupType:'Id',inquery:'workspace'},function(data,error)
        {
            console.log('Recieving at Client: ', data.length);
            var workspace_template = [];
            var number_of_workspaces = data.length;
            for(var i = 0; i < number_of_workspaces; i++)
            {
                var single_stage = {directory:data[i]};
                console.log(single_stage);
                workspace_template.push(single_stage);
            }
            render_workspaces(workspace_template);
            $('#selectWorkspace').modal('show');
        });

    }
</script>
<script type="text/javascript">
    function render_workspaces(json)
