    <link rel="stylesheet" type="text/css" href="../stylesheets/main_lfr.css" />

</head>

<body>

    <!-- Lfr Navigation Bar-->
    <nav class="navbar navbar-fixed-top" id="navBar">
        <div class="container-fluid">

            <div class="navbar-header">
                <a class="navbar-brand" >
                    <img height='75' alt="Brand" src="../images/index/specify_logo.svg">
                </a>
            </div>

            <div>
                <ul class="nav navbar-nav">
                    <li class="nav-item"><a class="nav-link" href="../"> <img src="../images/fluigi/homeButtonTransparent.png"> </a></li>
                    <li class="nav-item"><a class="nav-link" onclick="pushFileToEditor(LFR_start_editor,'LFR_start')"> <img src="../images/editor_control_icons/push_file.svg"> </a> </li>
                    <li class="nav-item"><a class="nav-link" onclick="refreshEditor(JSON.parse(localStorage.LFR_start_STRING),LFR_start_editor)"> <img src="../images/editor_control_icons/reload_file.svg"> </a> </li>
                    <li class="nav-item"><a class="nav-link" onclick="saveEditorContent(LFR_start_editor,'LFR_start')"> <img src="../images/editor_control_icons/save_file.svg"> </a> </li>

                </ul>
            </div>
        </div>
    </nav>


    <!-- UPLOAD BAR: UPLOAD LFR, UCF, MINT FILES -->
    <div id= "upload_bar" class="uploadbar" align="center">
        <div>

            <form id="uploadLFR_start"
                  enctype="multipart/form-data"
                  action="/api/LFR_start"
                  method="post">
                <div id="div1"><input id= "inputfile" class="btn btn-default" type="file" name="myLFR_start" /></div>

                <div id="div3">
                    <input id="uploadfile" class= "btn btn-default" type="submit" value="Upload LFR" name="submit" >
                    <span id = "status"></span>
                </div>

            </form>
        </div>

        <div>
            <label> *Refresh Page to load buttons</label>
        </div>

    </div>

    <!-- THE FOLLOWING CODE IS USED TO RENDER AND CONTROL THE EMBEDDED ACE EDITOR: -->
    <!-- BEGIN ACE EDITOR -->
    <div id="LFR_start_editor"></div> <!-- EDITOR DIV; EDITOR CONTENT GOES IN HERE-->
    <script src="/ace-master/src-noconflict/ace.js" type="text/javascript" charset="utf-8"></script>
    <script> var LFR_start_editor = ace.edit("LFR_start_editor"); LFR_start_editor.setTheme("ace/theme/mokokai"); LFR_start_editor.getSession().setMode("ace/mode/javascript"); </script>
    <!-- END ACE EDITOR -->

    <!-- ENDGAME DEPENDENCIES HERE -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>

</body>

