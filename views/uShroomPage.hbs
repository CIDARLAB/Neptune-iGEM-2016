<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
<script src="http://cdnjs.cloudflare.com/ajax/libs/jquery.form/3.51/jquery.form.min.js"></script>

<script>
    //Upload AJAX Forms
    {
        $(document).ready(function () {

            $('#uploadVerilog').submit(function () {


                $(this).ajaxSubmit({

                    error: function (xhr) {
                        status('Error: ' + xhr.status);
                    },
                    success: function (response) {
                        console.log(response);


                        var Verilogbutton = document.getElementById("input_verilog_file");
                        Verilogbutton.style.backgroundColor = "#2ecc71";
                        VerilogUploaded();

                    }
                });
                return false;
            });
            $('#uploadUCF').submit(function () {
                     $(this).ajaxSubmit({

                    error: function (xhr) {
                        status('Error: ' + xhr.status);
                    },
                    success: function (response) {
                        console.log(response);

                        location.reload();
                        var UCFbutton = document.getElementById("input_UCF_file");
                        UCFbutton.style.backgroundColor = "#2ecc71";
                        UCFUploaded();
                     }
                });
                return false;
            });
            $('#uploadMint').submit(function () {


                $(this).ajaxSubmit({

                    error: function (xhr) {
                        status('Error: ' + xhr.status);
                    },
                    success: function (response) {
                        console.log(response);

                        location.reload();
                        var MINTbutton = document.getElementById("input_mint_file");
                        MINTbutton.style.backgroundColor = "#2ecc71";
                        MINTUploaded();
                    }
                });
                return false;
            });
        });
    }
</script>

