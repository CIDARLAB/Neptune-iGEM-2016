<script>
    //Upload AJAX Forms
    {
        $(document).ready(function () {

            $('#uploadVerilog').submit(function () {
                $("#statusVerilog").empty().text("File is uploading...");

                $(this).ajaxSubmit({

                    error: function (xhr) {
                        status('Error: ' + xhr.status);
                    },
                    success: function (response) {
                        console.log(response);
                        $("#statusVerilog").empty().text(response);
                        location.reload();
                        UCFUploaded();
                        updateIndicators();
                    }
                });
                return false;
            });
            $('#uploadUCF').submit(function () {
                $("#statusUCF").empty().text("File is uploading...");

                $(this).ajaxSubmit({

                    error: function (xhr) {
                        status('Error: ' + xhr.status);
                    },
                    success: function (response) {
                        console.log(response);
                        $("#statusUCF").empty().text(response);
                        location.reload();
                        verilogUploaded();
                        updateIndicators();
                    }
                });
                return false;
            });
        });
    }
</script>

